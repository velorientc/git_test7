msgstr "Веб сервер"

msgid "Port:"
msgstr "Порт:"

msgid "Status:"
msgstr "Состояние:"

msgid "Start"
msgstr "Запуск"

msgid "Settings"
msgstr "Настройки"

msgid "<unspecified>"
msgstr "<не указано>"

msgid "&True"
msgstr "&Да"

msgid "&False"
msgstr "&Нет"

